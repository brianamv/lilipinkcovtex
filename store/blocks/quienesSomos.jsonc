  "rich-text#nuestroPilar": {
    "props": {
      "text": "NUESTRO PILAR",
      "blockClass": "titulo-landing"
    }
  },
  "rich-text#soyLili": {
    "props": {
      "text": "Yo soy Lili…",
      "blockClass": "texto-landing"
    }
  },
  "rich-text#soyLiliInfo": {
    "props": {
      "text": "Soy una mujer joven, divertida, alegre, expresiva, buena amiga, espontanea, cercana, curiosa, segura, independiente, que navega confiada en el mundo digital. Me gusta mi cuerpo y quiero que todas amen el suyo, soy incluyente y diversa.",
      "blockClass": "texto-landing"
    }
  },
  "rich-text#sobreLiliPink": {
    "props": {
      "text": "SOBRE LILIPINK.",
      "blockClass": "titulo-landing"
    }
  },
  "rich-text#sobreLiliPinkInterno": {
    "props": {
      "text": "Estamos ubicados en 6 países con más de 50 tiendas: México, Guatemala, Costa Rica, Panamá, Ecuador y Perú.En Colombia, contamos con más de 200 tiendas propias, más de 100 franquicias y más de 50 tiendas de nuestra marca YOI..",
      "blockClass": "texto-landing"
    }
  },
