{
  "store.custom.quienes-somos": {
    "blocks": [
      "flex-layout.row#quienesSomos",
      "flex-layout.row#textos1",
      "flex-layout.row#Ideologia-Fotos",
      "flex-layout.row#textolanding2",
      "flex-layout.row#SucrusalGeneral",
      "flex-layout.row#clic-vacante1"

    ]
  },

  "flex-layout.row#textos1": {
    "children": [
      "flex-layout.col#textos1"
    ],
    "props": {
      "marginTop": 6,
      "marginBottom": 6,
      "blockClass":"margenLanding"
    }
  },
  "flex-layout.col#textos1": {
    "children": [
      "rich-text#QuienesSomos",
      "rich-text#QuienesSomosInterno",
      "rich-text#ideologia",
      "rich-text#ideologiaInterna"
    ]
  },
  "flex-layout.row#textolanding2": {
    "props":{
      "blockClass":"margenLanding"
    },
    "children": [
      "flex-layout.col#textolanding2"
    ]
  },
  "flex-layout.col#textolanding2": {
    "children": [
      "rich-text#nuestroPilar",
      "rich-text#soyLili",
      "rich-text#soyLiliInfo",
      "rich-text#sobreLiliPink",
      "rich-text#sobreLiliPinkInterno"
    ]
  },
  "flex-layout.row#quienesSomos": {
    "props": {
      "verticalAlign": "middle",
      "horizontalAlign": "center",
      "blockClass":"margenLanding"
    },
    "children": [
      "flex-layout.col#quienes-somos-1", "flex-layout.col#quienes-somos-2", "flex-layout.col#quienes-somos-3"

    ]
  },
  "flex-layout.col#quienes-somos-1": {
    "props": {
      "verticalAlign": "middle",
      "horizontalAlign": "center"
    },
    "children": [
      "image#quienessomos1"
    ]
  },
  "image#quienessomos1": {
    "props": {
      "src": "assets/quieneSomos3.jpg",
      "blockClass": "imagenes-quienes-somos"
    }
  },
  "flex-layout.col#quienes-somos-2": {
    "props": {
      "verticalAlign": "middle",
      "horizontalAlign": "center"
