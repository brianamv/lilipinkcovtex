{
  "element-wrapper.footer-lilipink": {
    "children": ["footer"],
    "props": {
      "blockClass": "header-lilipink"
    }
  },
  "footer": {
    "blocks": ["footer-layout.desktop", "footer-layout.mobile#lili"]
  },
  "footer-layout.desktop": {
    "children": [
      "flex-layout.row#footer-1-desktop",
      "flex-layout.row#footer-2-desktop",
      "flex-layout.row#footer-3-desktop"
    ]
  },
  "flex-layout.row#footer-1-desktop": {
    "children": [
      "footer-spacer",
      "vtex.menu@2.x:menu#Products",
      "footer-spacer",
      "vtex.menu@2.x:menu#footer-decoration",
      "footer-spacer",
      "vtex.menu@2.x:menu#footer-Atenci√≥n-al-cliente",
      "footer-spacer"

    ],
    "props": {
      "blockClass": "menu-row",
      "paddingTop": 6,
      "paddingBottom": 6
    }
  },



  "flex-layout.row#footer-2-desktop": {
    "children": [

      "flex-layout.row#imagenes-tarjetas",
